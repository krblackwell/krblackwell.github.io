{
  "version": "0.2",
  "language": "en",
  "dictionaries": ["python", "custom-dictionary"],
  "dictionaryDefinitions": [
    {
      "name": "python",
      "path": "node_modules/@cspell/dict-python/cspell-ext.json"
    },
    {
      "name": "custom-dictionary",
      "path": "./cspell-custom-dictionary.txt"
    }
  ],
  "ignorePaths": [
    "**/node_modules/**",
    "**/__pycache__/**"
  ],
  "files": ["**/*.md", "**/*.mdx"]
}
